{
	"name": "skeeks/cms-i18n-db",
	"description": "Storage translations in a database component for SkeekS CMS",
	"keywords": ["yii", "skeeks", "framework", "component", "i18n"],
    	"homepage": "https://cms.skeeks.com/",
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"support": {
        "issues": "https://skeeks.com/"
    },
	"authors": [
		{
			"name": "Semenov Alexander",
			"email": "semenov@skeeks.com"
		}
	],
	"require": {
		"skeeks/cms": "^5.0 || dev-master"
	},
    "autoload": {
        "psr-4": {
            "skeeks\\cms\\i18nDb\\": "src/"
        }
    },
    "extra": {
        "config-plugin": {
            "web": [
                "src/config/common.php"
            ],
            "console": [
                "src/config/common.php",
                "src/config/console.php"
            ]
        }
    }
}
